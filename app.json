{
  "expo": {
    "name": "booking-lapangan",
    "owner": "ahmadsyibaihi",
    "slug": "booking-lapangan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "/assets/icon.png",
    "scheme": "bookinglapangan",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.syibaihi.bookinglapangan",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "bookinglapangan"
            ]
          }
        ]
      },
      "appStoreId": "200705"
    },
    "android": {
      "package": "com.syibaihi.bookinglapangan", 
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "bookinglapangan",
              "host": "oauth2redirect",
              "pathPrefix": "/google"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
